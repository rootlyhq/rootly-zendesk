{
  "name": "Rootly",
  "author": {
    "name": "Rootly",
    "email": "support@rootly.com",
    "url": "https://www.rootly.com/"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      }
    }
  },
  "domainWhitelist": [
    "api.rootly.com",
    "rootly.com"
  ],
  "parameters": [
    {
      "name": "apiUrl",
      "required": true,
      "secure": true,
      "default": "https://api.rootly.com/v1",
      "type": "hidden"
    },
    {
      "name": "apiKey",
      "required": true,
      "secure": true,
      "type": "text"
    }
  ],
  "version": "1.0",
  "frameworkVersion": "2.0"
}